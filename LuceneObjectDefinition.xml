<?xml version="1.0"?>
<ObjectDefinition xmlns:xi="http://www.w3.org/2001/XInclude" type="x3dv">
    <Document type="default">
        <xi:include href="../../../../../../../Services/Object/LuceneDataSource.xml"/>
        <DataSource type="JDBC" action="append">
            <Query>
                SELECT 'Repoplugin' AS x3dvstring FROM object_data
                WHERE obj_id IN (?)
            </Query>
            <Param format="list" type="int" value="objId"/>
            <Field store="YES" index="ANALYZED" column="x3dvstring" type="text" name="propertyMedium"/>
        </DataSource>
    </Document>
</ObjectDefinition>
